{
  "name": "@karya_app1/rain-js",
  "version": "0.2.7",
  "description": "A React library with useful hooks and UI components",
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:clean": "rm -rf node_modules/.cache/storybook",
    "build:storybook": "storybook build",
    "deploy-storybook": "storybook-to-aws-s3 --bucket-path=rain-js-storybook --bucket-region=us-east-1",
    "release": "npm publish --access public",
    "postpublish": "python3 deploy.py --bucket rain-js-storybook"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/test": "8.6.14",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "8.6.14",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  }
}
